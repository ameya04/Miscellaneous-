#include<bits/stdc++.h>
using namespace std;

int main(){
    int a[2][3]={{2,3,4},{3,4,5}};
    int b[2][3]={{6,7,8},{7,8,9}};
    int sum[2][3];
    
    for (int i=0;i<2;i++){
        for (int j=0;j<3;j++){
            sum[i][j]=a[i][j]+b[i][j];    
    }
}
for(int i=0;i<2;i++){
    for (int j=0;j<3;j++){
       cout<<sum[i][j]<<" ";
    }cout<<endl;
}}
