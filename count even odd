#include<bits/stdc++.h>
using namespace std;

int main(){
  int array[8]={1,2,3,4,5,6,7,8};
  int evenCount=0;
      int oddCount=0;
  for(int i=0;i<8; i++){
      
      if(array[i]%2==0){
          evenCount=evenCount+1;
          ;
      }
      else {oddCount=oddCount+1;}
 
  }cout<<"even: "<< evenCount<<" odd: "<<oddCount<<endl;
  
    return 0;  
}
